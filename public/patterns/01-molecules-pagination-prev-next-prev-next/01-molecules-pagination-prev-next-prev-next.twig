<nav class="qh__prevnext" aria-label="Events">
  <ul class="qh__prevnext__list">
    <li class="qh__prevnext__item qh__prevnext__prev">{%- if prev_url -%}
      <a class="qh__prevnext__link" href="{{ prev_url }}">
        <span class="small-text qh__prevnext__label">Previous</span>
        <span class="qh__prevnext__title">{{ prev_title }}</span>
      </a>
    {%- endif -%}</li>
    <li class="qh__prevnext__item qh__prevnext__next">{%- if next_url -%}
      <a class="qh__prevnext__link" href="{{ next_url }}">
        <span class="small-text qh__prevnext__label">Next</span>
        <span class="qh__prevnext__title">{{ next_title }}</span>
      </a>
    {%- endif -%}</li>
  </ul>
</nav>
