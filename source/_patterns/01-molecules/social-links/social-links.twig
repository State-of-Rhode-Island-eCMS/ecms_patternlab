<nav class="qh__social-links" aria-label="Social Media">
  <ul class="qh__social-links__list qh__u__no-external">
    <li class="qh__social-links__item">
      {# Facebook social "card" #}
      <a class="qh__social-links__link" href="#" rel="external" title="{{ "Follow us on Facebook"|t }}">
        <span class="qh__icon">
          {% include ('@icons/social-facebook.svg') %}
        </span>
        <span class="visually-hidden">{{ "Facebook"|t }}</span>
      </a>
    </li>
    <li>
      {# Twitter social "card" #}
      <a class="qh__social-links__link" href="#" rel="external" title="{{ "Follow us on Twitter"|t }}">
        <span class="qh__icon">
          {% include ('@icons/social-twitter.svg') %}
        </span>
        <span class="visually-hidden">{{ "Twitter"|t }}</span>
      </a>
    </li>
    <li>
      {# LinkedIn social "card" #}
      <a class="qh__social-links__link" href="#" rel="external" title="{{ "Follow us on LinkedIn"|t }}">
        <span class="qh__icon">
          {% include ('@icons/social-linkedin.svg') %}
        </span>
        <span class="visually-hidden">{{ "LinkedIn"|t }}</span>
      </a>
    </li>
  </ul>
</nav>
