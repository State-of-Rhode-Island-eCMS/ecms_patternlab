        {% include "@organisms/navigation-minor/navigation-minor.twig" with {
            items: minor_items,
            level_one_link: level_one_link
          }
        %}
      </div>

      {% if page.sidebar_middle %}
        <aside class="qh-layout__sidebar-middle" role="complementary">
          {{ page.sidebar_middle }}
        </aside>
      {% endif %}

      {% if page.sidebar_bottom %}
