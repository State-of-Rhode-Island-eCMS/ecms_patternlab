    #
    # Ocean
    #
    "ocean--bright": {
      "name": "Ocean State bright",
      "hue": "light blue green",
      "hsl": "hsl(160, 100%, 74%)",
      "needs": "dark-text"
    },
    "ocean": {
      "name": "Ocean State",
