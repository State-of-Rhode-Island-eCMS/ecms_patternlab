        "nav-main": [
          { "fnName": "fg", "colorName": "quahog--light" },
          { "fnName": "bg", "colorName": "navy" },
          { "fnName": "link", "colorName": "quahog--light" },
          { "fnName": "link--hover", "colorName": "white" },
          # Hover bg used for bg of link AND for border top of current parent
          # Should be the same color as the Header BG to look like a notch
          { "fnName": "link--hover__bg", "colorName": "quahog--darker" },
          { "fnName": "link--focus", "colorName": "hope-gold" },
          { "fnName": "link--active", "colorName": "hope-gold" },
          { "fnName": "link__sub--hover__bg", "colorName": "quahog--dark--trans15" },
          { "fnName": "icon__bg", "colorName": "ocean" },
          #{ "fnName": "icon__fg", "colorName": "navy" },
          { "fnName": "icon__bg--hover", "colorName": "hope-gold" },
          { "fnName": "icon__fg--hover", "colorName": "navy--dark" }
        ],
        "default": [
          { "fnName": "fg", "colorName": "coffee-milk--light" },
          { "fnName": "bg", "colorName": "quahog--dark" },
          { "fnName": "border", "colorName": "white--trans25" },
          { "fnName": "link", "colorName": "ocean" },
          { "fnName": "link--hover", "colorName": "hope-gold" },
          { "fnName": "link--focus", "colorName": "ocean" },
          { "fnName": "link--visited", "colorName": "navy--wrm-lgt" },
          { "fnName": "link--active", "colorName": "calamari" },
          { "fnName": "hx__fg", "colorName": "coffee-milk" },
