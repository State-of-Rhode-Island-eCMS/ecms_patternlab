          #{ "fnName": "button__fg", "colorName": "hope-gold" },
          #{ "fnName": "button--hover__bg", "colorName": "navy--dark" },
          #{ "fnName": "button--hover__fg", "colorName": "ocean" }
        ],
        "icon": [
          #{ "fnName": "pdf", "colorName": "rhody-red" },
          { "fnName": "word", "colorName": "ocean" },
          #{ "fnName": "excel", "colorName": "green-animals" },
          { "fnName": "generic", "colorName": "coffee-milk" },
          { "fnName": "download", "colorName": "quahog--light" }
        ],
        "nav-minor": [
          { "fnName": "border", "colorName": "navy" },
          { "fnName": "fg", "colorName": "quahog--light" },
          { "fnName": "bg", "colorName": "navy--trans50" },
          { "fnName": "link", "colorName": "coffee-milk--light" },
          { "fnName": "link__bg", "colorName": "transparent" },
          { "fnName": "link--hover", "colorName": "white" },
          { "fnName": "link--hover__bg", "colorName": "quahog--dark" },
          { "fnName": "link--focus", "colorName": "hope-gold" },
          { "fnName": "link--active", "colorName": "hope-gold" },
          { "fnName": "sub__bg", "colorName": "quahog--dark--trans33" }
        ],
        #
        # Notification colors usually fall back to the same palette as the Light theme
        #
