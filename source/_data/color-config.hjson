  "palettes": {
    #
    # NOTE: The first color palette is defined as the DEFAULT color palette
    #
    # Structure:
    # "scarborough" -> This is the Theme slug
    # "humanName"   -> Used by PHP/Twig to build the drop down menu in the admin
    # "values"      -> Collection of blocks of functional color definitions
    # block-name    -> Like "official": This is the component slug
    # "fnName"      -> The "functional name" of a color slug, like "fg" for foreground
    # "colorName"   -> A color name key that MUST match a color defined above
    #
    "scarborough": {
      "humanName": "Scarborough Beach",
      "values": {
        "official": [
          { "fnName": "fg", "colorName": "quahog--light" },
          { "fnName": "bg", "colorName": "navy--dark" },
          { "fnName": "link", "colorName": "white" },
          { "fnName": "link--hover", "colorName": "calamari" },
          { "fnName": "link--focus", "colorName": "calamari" }
        ],
        "header": [
          { "fnName": "fg", "colorName": "white" },
          { "fnName": "bg", "colorName": "navy" },
          { "fnName": "link", "colorName": "white" },
          { "fnName": "link--hover", "colorName": "ocean" },
          { "fnName": "social__link", "colorName": "ocean" },
          # Hover should use the accent or violator color
          { "fnName": "social__link--hover", "colorName": "hope-gold" }
        ],
        "nav-main": [
          { "fnName": "fg", "colorName": "quahog--dark" },
          { "fnName": "bg", "colorName": "ocean" },
          { "fnName": "link", "colorName": "quahog--darker" },
          { "fnName": "link--hover", "colorName": "navy" },
          # Hover bg used for bg of link AND for border top of current parent
          # Should be the same color as the Header BG to look like a notch
          { "fnName": "link--hover__bg", "colorName": "white" },
          { "fnName": "link--focus", "colorName": "hope-gold" },
          { "fnName": "link--active", "colorName": "hope-gold" },
          { "fnName": "link__sub--hover__bg", "colorName": "ocean--trans25" },
          { "fnName": "icon__bg", "colorName": "white" },
          { "fnName": "icon__fg", "colorName": "navy" },
          { "fnName": "icon__bg--hover", "colorName": "ocean" },
          { "fnName": "icon__fg--hover", "colorName": "navy" }
        ],
        "default": [
          { "fnName": "fg", "colorName": "quahog" },
          { "fnName": "bg", "colorName": "white" },
          { "fnName": "border", "colorName": "quahog--dark--trans15" },
          { "fnName": "link", "colorName": "navy--sat" },
          { "fnName": "link--hover", "colorName": "navy--sat-lgt" },
          { "fnName": "link--focus", "colorName": "navy" },
          { "fnName": "link--visited", "colorName": "navy--warm" },
          { "fnName": "link--active", "colorName": "calamari" },
          { "fnName": "hx__fg", "colorName": "quahog--medium" },
          { "fnName": "violator", "colorName": "calamari" },
          # Accent pair, bg and fg
          { "fnName": "accent", "colorName": "hope-gold" },
          { "fnName": "accent__fg", "colorName": "quahog--dark" },
          # text__accent currently used for bullet mark color
          { "fnName": "text__accent", "colorName": "quahog--medium" },
          { "fnName": "button__bg", "colorName": "navy" },
          { "fnName": "button__fg", "colorName": "hope-gold" },
          { "fnName": "button--hover__bg", "colorName": "navy--dark" },
          { "fnName": "button--hover__fg", "colorName": "ocean" }
        ],
        #
        # System icons used throughout
        # but right now only in the File List component
        #
        "icon": [
          { "fnName": "pdf", "colorName": "rhody-red" },
          { "fnName": "word", "colorName": "navy" },
          { "fnName": "excel", "colorName": "green-animals" },
          { "fnName": "generic", "colorName": "ocean" },
          { "fnName": "download", "colorName": "quahog--medium" }
        ],
        #
        # Left sidebar navigation component
        #
        "nav-minor": [
          { "fnName": "border", "colorName": "ocean" },
          { "fnName": "fg", "colorName": "quahog--darker" },
          { "fnName": "bg", "colorName": "ocean--trans25" },
          { "fnName": "link", "colorName": "navy" },
          { "fnName": "link__bg", "colorName": "transparent" },
          { "fnName": "link--hover", "colorName": "navy--dark" },
          { "fnName": "link--hover__bg", "colorName": "ocean--trans25" },
          { "fnName": "link--focus", "colorName": "calamari" },
          { "fnName": "link--active", "colorName": "calamari" },
          { "fnName": "sub__bg", "colorName": "white--trans25" }
        ],
        #
        # Site Notifications component
        #
        "notification": [
          { "fnName": "fg", "colorName": "navy--dark" },
          { "fnName": "bg", "colorName": "hope-gold" },
          { "fnName": "icon", "colorName": "navy" },
          { "fnName": "link", "colorName": "navy--dark" },
          { "fnName": "link--hover", "colorName": "navy--sat-lgt" },
          { "fnName": "link--focus", "colorName": "navy--dark" },
          { "fnName": "link--active", "colorName": "calamari" },
          # Hover will always reverse these button colors
          { "fnName": "button__fg", "colorName": "navy" },
          { "fnName": "button__bg", "colorName": "white" },
          { "fnName": "border", "colorName": "quahog--dark--trans33" }
        ],
        #
        # Only the icon foreground and background is colorable
        # Other colors are consistently coffee-milk
        #
        "accordion": [
          { "fnName": "icon__fg", "colorName": "navy--dark" },
          { "fnName": "icon__bg", "colorName": "calamari" }
        ],
        #
        # Do not use transparent colors for "bar" and "fg"
        # "bar" is applied to the background shape behind the icon as well,
        # and we will see transparent colors multiply over eachother
        #
        "attention-card": [
          { "fnName": "fg", "colorName": "navy--dark" },
          { "fnName": "bar", "colorName": "ocean" },
          { "fnName": "icon", "colorName": "navy--dark" }
        ],
        #
        # Consistent from theme to theme.
        # Always grays with a pop of color only in the "border-top"
        #
        "footer": [
          { "fnName": "border-top", "colorName": "ocean" },
          { "fnName": "fg", "colorName": "white" },
          { "fnName": "bg", "colorName": "quahog" },
          { "fnName": "copyright__bg", "colorName": "quahog--darker" }
        ],
        #
        # These sub-themes need to provide the light and dark variations
        # In dark theme definitions, they need only contain the dark theme colors
        #
        "primary": [
          { "fnName": "fg", "colorName": "white" },
          { "fnName": "bg", "colorName": "navy" },
          { "fnName": "fg--dark", "colorName": "white" },
          { "fnName": "bg--dark", "colorName": "navy--trans50" }
        ],
        "primary--light": [
          { "fnName": "fg", "colorName": "navy--dark" },
          { "fnName": "bg", "colorName": "ocean" },
          { "fnName": "fg--dark", "colorName": "white" },
          { "fnName": "bg--dark", "colorName": "ocean--trans25" }
        ]
      }
    },
    #
    # A Dark theme is defined here with the "--dark" keyword appended to the original slug
    #
    # If a functional color pair is commented out, it is not being used, and therefore,
    # in CSS, the value will come from the default color palette
